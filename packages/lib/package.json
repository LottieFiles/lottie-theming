{
  "name": "@lottiefiles/lottie-theming",
  "version": "2.0.1",
  "description": "Lottie theming library",
  "keywords": [
    "lottie",
    "lottiefiles",
    "theme",
    "theming",
    "motion design",
    "web",
    "web animation",
    "animation"
  ],
  "publishConfig": {
    "access": "public"
  },
  "browser": "./dist/lottie-theming.umd.js",
  "main": "./dist/lottie-theming.cjs.js",
  "module": "./dist/lottie-theming.es.js",
  "exports": {
    ".": {
      "import": "./dist/lottie-theming.umd.js",
      "require": "./dist/lottie-theming.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LottieFiles/lottie-theming.git"
  },
  "homepage": "https://github.com/LottieFiles/lottie-theming#readme",
  "bugs": {
    "url": "https://github.com/LottieFiles/lottie-theming/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Karam Ali"
  },
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "release": "yarn build && yarn changeset version && yarn changeset publish"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-strip": "2.1.0",
    "@rollup/plugin-typescript": "8.2.5",
    "@types/jest": "26.0.24",
    "@types/node": "^16.4.7",
    "@types/react": "^17",
    "@types/testing-library__jest-dom": "^5",
    "jest": "27.0.6",
    "rollup-plugin-dts": "3.0.2",
    "rollup-plugin-esbuild": "4.5.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-typescript2": "0.30.0",
    "semantic-release": "^17.4.2",
    "shx": "0.3.3",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5",
    "vite": "2.4.4"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@lottiefiles/lottie-js": "0.2.1"
  }
}
