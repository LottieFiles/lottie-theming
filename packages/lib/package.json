{
  "name": "@lottiefiles/lottie-theming",
  "version": "0.0.1",
  "description": "Lottie theming library",
  "keywords": [
    "lottie",
    "theme",
    "theming"
  ],
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/lottie-theming.umd.js",
    "module": "./dist/lottie-theming.es.js",
    "exports": {
      ".": {
        "import": "./dist/lottie-theming.umd.js",
        "require": "./dist/lottie-theming.es.js"
      }
    },
    "types": "dist/lottie-theming.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LottieFiles/lottie-theming.git"
  },
  "homepage": "https://github.com/LottieFiles/lottie-theming#readme",
  "bugs": {
    "url": "https://github.com/LottieFiles/lottie-theming/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Karam Ali"
  },
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "shx rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^16.4.7",
    "shx": "0.3.3",
    "typescript": "4.3.5",
    "vite": "2.4.4"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "cross-fetch": "3.1.4"
  }
}
